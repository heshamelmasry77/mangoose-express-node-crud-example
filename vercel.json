{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/getalldevices",
      "dest": "app.js"
    },
    {
      "src": "/createdevice",
      "dest": "app.js"
    },
    {
      "src": "/device/:id",
      "dest": "app.js"
    }
  ]
}
